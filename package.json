        "start": "cross-env HTTPS=true SSL_CRT_FILE=cert.pem SSL_KEY_FILE=key.pem react-scripts start",
        "start-development": "cross-env HTTPS=true SSL_CRT_FILE=cert.pem && ./node_modules/.bin/env-cmd -f .env.development react-scripts start",
        "start-staging": "cross-env HTTPS=true SSL_CRT_FILE=cert.pem && ./node_modules/.bin/env-cmd -f .env.staging react-scripts start",
        "start-production": "./node_modules/.bin/env-cmd -f .env.production react-scripts start",
        "build": "react-scripts build",
        "build-development": "./node_modules/.bin/env-cmd -f .env.development react-scripts build",
        "build-staging": "./node_modules/.bin/env-cmd -f .env.staging react-scripts build",
        "build-production": "./node_modules/.bin/env-cmd -f .env.production react-scripts build",
        "run:dev": "run build-development && serve -l 3016 -s build",
        "serve": "serve -l 3016 -s deployed",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
